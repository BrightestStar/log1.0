<h2>New</h2>

<%= simple_form_for(@log) do |f| %>

<div class="form-group">
    <%= f.label :标题 %><br>
    <%= f.text_field :title, class: "form-control" %>
</div>

<div class="form-group">
    <%= f.label :内容 %><br>
    <%= f.text_field :body, class: "form-control" %>
</div>

<div class="group">
  <%= f.submit "Submit", class: "btn btn-primary" %>
</div>

<% end %>
